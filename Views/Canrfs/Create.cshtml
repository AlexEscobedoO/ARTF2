@model ARTF2.Models.Canrf

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Crear Cancelación</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NumacuofsolNavigator" class="control-label"></label>
                <select asp-for="NumacuofsolNavigator" class ="form-control" asp-items="ViewBag.NumacuofsolNavigator"></select>
            </div>
            <div class="form-group">
                <label asp-for="Fechaofcan" class="control-label"></label>
                <input asp-for="Fechaofcan" class="form-control" />
                <span asp-validation-for="Fechaofcan" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Juscan" class="control-label"></label>
                <textarea asp-for="Juscan" class="form-control" rows="5" maxlength="200" ></textarea>
                <span asp-validation-for="Juscan" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Obscan" class="control-label"></label>
                <textarea asp-for="Obscan" class="form-control" rows="5" maxlength="200" ></textarea>
                <span asp-validation-for="Obscan" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NameFichaDoc" class="control-label"></label>
                <input asp-for="NameFichaDoc" class="form-control" />
                <span asp-validation-for="NameFichaDoc" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Fichacan" class="control-label"></label>
                <input type="file" asp-for="Fichacan" class="form-control" />
                <span asp-validation-for="Fichacan" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Homoclavecan" class="control-label"></label>
                <input asp-for="Homoclavecan" class="form-control" />
                <span asp-validation-for="Homoclavecan" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Fechacan" class="control-label"></label>
                <input asp-for="Fechacan" class="form-control" />
                <span asp-validation-for="Fechacan" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-success" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-controller="Insrfs" asp-action="Index" class="btn btn-info">Volver a la lista</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
